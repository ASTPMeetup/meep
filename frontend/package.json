    "react-router-dom": "5.0.1",
    "react-scripts": "3.0.1",
    "validator": "8.0.0",
    "webpack": "4.34.0",
    "webpack-cli": "3.3.4"
  },
  "optionalDependencies": {
    "fsevents": "^2.0.7"
  },
  "devDependencies": {
